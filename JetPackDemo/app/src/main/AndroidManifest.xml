<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dream.jetpackdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackDemo">
        <activity
            android:name="com.dream.jetpackdemo.databinding.ErdaiActivity"
            android:exported="false" />
        <activity
            android:name="com.dream.jetpackdemo.databinding.DataBindingActivity"
            android:exported="false" />
        <activity
            android:name="com.dream.jetpackdemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".lifecycle.LifeCycleActivity"
            android:exported="false" />
        <activity
            android:name=".lifecycle.MyLifecycleOwnerImplActivity"
            android:exported="false" />
        <activity
            android:name=".livedata.LiveDataActivity"
            android:exported="false" />
        <activity
            android:name=".livedata.LiveData2Activity"
            android:exported="false" />
        <activity android:name=".viewmodel.ViewModelActivity"
            android:exported="false"/>
    </application>

</manifest>